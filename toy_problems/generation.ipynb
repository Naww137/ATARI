    "def noisify(y_true, noise):\n",
    "    y_exp_mean = y_true + np.random.randn(y_true.shape)\n",
    "    y_exp_std  = noise * np.ones(y_true.shape)\n",
    "    return y_exp_mean, y_exp_std"
